{
    "name": "sspat/doctrine-nullable-embeddables",
    "description": "Patches doctrine/orm to allow usage of nullable embeddables",
    "type": "library",
    "version": "2.7",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "nullable",
        "embeddable"
    ],
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "cweagans/composer-patches": "^1.6",
        "doctrine/orm": "2.7"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "phpunit/phpunit": "^8.4"
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineNullableEmbeddables\\Tests\\": "tests/",
            "Doctrine\\Tests\\": "vendor/doctrine/orm/tests/Doctrine/Tests"
        }
    },
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Allow nullable embeddables": "https://raw.githubusercontent.com/sspat/doctrine-nullable-embeddables/v2.7.0/patch/nullable_embeddables.patch"
            }
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist",
            "doctrine/orm": "source"
        },
        "sort-packages": true
    }
}
