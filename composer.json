{
    "name": "sspat/doctrine-nullable-embeddables",
    "description": "Patches doctrine/orm to allow usage of nullable embeddables",
    "type": "library",
    "version": "2.7",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "nullable",
        "embeddable"
    ],
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "cweagans/composer-patches": "^1.6",
        "doctrine/orm": "2.7"
    },
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Allow nullable embeddables": "https://raw.githubusercontent.com/sspat/doctrine-nullable-embeddables/3dd4222fe52968955eec7bb5956cc8164b489c44/patch/nullable_embeddables.patch"
            }
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist",
            "doctrine/orm": "source"
        },
        "sort-packages": true
    }
}
