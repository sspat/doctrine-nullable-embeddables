{
    "name": "sspat/doctrine-nullable-embeddables",
    "description": "Patches doctrine/orm to allow usage of nullable embeddables",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "nullable",
        "embeddable"
    ],
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "cweagans/composer-patches": "^1.6|^1.7",
        "doctrine/orm": "2.9.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineNullableEmbeddables\\Tests\\": "tests/",
            "Doctrine\\Tests\\": "vendor/doctrine/orm/tests/Doctrine/Tests"
        }
    },
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Allow nullable embeddables": "https://raw.githubusercontent.com/sspat/doctrine-nullable-embeddables/v2.9.6/patch/nullable_embeddables.patch"
            }
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "cweagans/composer-patches": true
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
